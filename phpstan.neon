parameters:
    level: max
    paths:
        - config
        - src
    ignoreErrors:
        -
            message: '#^Cannot call method isProduction\(\) on mixed\.$#'
            count: 1
            path: config/fluent.php
    tmpDir: .cache/phpstan

includes:
    - vendor/phpstan/phpstan/conf/bleedingEdge.neon
