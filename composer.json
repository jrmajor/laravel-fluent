{
    "name": "jrmajor/laravel-fluent",
    "description": "Fluent translations for Laravel",
    "license": "MIT",
    "keywords": [
        "translator",
        "translation",
        "i18n",
        "internationalization",
        "l10n",
        "localization",
        "locale",
        "language",
        "plural",
        "gender",
        "ftl",
        "mozilla"
    ],
    "authors": [
        {
            "name": "Jeremiasz Major",
            "email": "jrh.mjr@gmail.com"
        }
    ],
    "homepage": "https://github.com/jrmajor/laravel-fluent",
    "require": {
        "php": "~8.0.0 || ~8.1.0",
        "jrmajor/fluent": "^0.4.0",
        "laravel/framework": "^8.0 || ^9.0"
    },
    "require-dev": {
        "jrmajor/cs": "^0.3.4",
        "orchestra/testbench": "^6.24 || ^7.0",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Major\\Fluent\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Major\\Fluent\\Laravel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Major\\Fluent\\Laravel\\FluentServiceProvider"
            ]
        }
    }
}
